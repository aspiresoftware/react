{
  "name": "nativewebdesktop",
  "version": "0.0.1",
  "private": true,
  "main": "./desktop/main.js",
  "scripts": {
    "run-web": "webpack-dev-server --port 3000 --open --config web/webpack.config.dev.js --inline --hot --colors --quiet --history-api-fallback",
    "prod-web": "webpack --config web/webpack.config.prod.js --progress --colors",
    "start-electron": "electron .",
    "remove-bundle": "del-cli ./desktop/bundle.js",
    "run-desktop": "yarn run remove-bundle && yarn run prod-web && yarn run start-electron",
    "pack-linux": "yarn run remove-bundle && yarn run prod-web && electron-packager . --overwrite --asar=true--platform=linux --arch=x64 --icon=./src/assets/icon.png --prune=true --out=release-builds-linux",
    "pack-windows": "yarn run remove-bundle && yarn run prod-web && electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=src/assets/icon.ico --prune=true --out=release-builds-win --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='ReactNativeWebDesktopDemo'",
    "pack-mac": "yarn run remove-bundle && yarn run prod-web && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=src/assets/icon.icns --prune=true --out=release-builds-mac"
  },
  "dependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-react-native": "2.1.0",
    "babel-preset-react-native-stage-0": "1.0.1",
    "bootstrap": "^3.3.7",
    "connectivity": "^1.0.0",
    "css-loader": "^0.28.7",
    "del-cli": "1.1.0",
    "electron": "1.7.10",
    "electron-google-oauth": "^2.0.0",
    "electron-packager": "10.1.0",
    "eslint": "4.13.1",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.5.1",
    "file-loader": "1.1.6",
    "firebase": "^4.8.1",
    "font-loader": "^0.1.2",
    "fs": "^0.0.1-security",
    "is-offline": "^2.1.0",
    "is-online": "^7.0.0",
    "jest": "21.2.1",
    "react": "16.2.0",
    "react-content-loader": "^1.7.3",
    "react-detect-offline": "^1.0.6",
    "react-dom": "16.2.0",
    "react-ga": "^2.4.0",
    "react-hot-loader": "3.1.3",
    "react-loader-spinner": "^1.4.7",
    "react-native": "0.51.0",
    "react-native-device-info": "^0.13.0",
    "react-native-elements": "^0.18.5",
    "react-native-web": "0.1.16",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "16.0.0",
    "rxjs": "^5.5.6",
    "style-loader": "^0.19.1",
    "tcomb-form-native": "^0.6.11",
    "url-loader": "^0.6.2",
    "validate.js": "^0.12.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7",
    "webpack-target-electron-renderer": "^0.4.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.3",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-preset-react-native-stage-0": "1.0.1",
    "electron": "1.7.10",
    "eslint": "4.13.1",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-react": "7.5.1",
    "jest": "21.2.1",
    "react-hot-loader": "3.1.3",
    "react-test-renderer": "16.0.0",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.7"
  },
  "jest": {
    "preset": "react-native"
  }
}
